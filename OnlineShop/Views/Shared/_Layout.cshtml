    <!--Slider-Starts-Here-->
    <script src="/Asset/client/js/responsiveslides.min.js"></script>
    <script>
        // You can also use "$(window).load(function() {"
        $(function () {
            // Slideshow 4
            $("#slider4").responsiveSlides({
                auto: true,
                pager: true,
                nav: true,
                speed: 500,
                namespace: "callbacks",
                before: function () {
                    $('.events').append("<li>before event fired.</li>");
                },
                after: function () {
                    $('.events').append("<li>after event fired.</li>");
                }
            });

        });
    </script>
    <!--End-slider-script-->
    <!--about-starts-->
    @RenderSection("about", required: false)

    @RenderSection("productCategory", required: false)
    <!--about-end-->
    <!--product-starts-->
    @RenderBody()
    <!--product-end-->
    <!--information-starts-->
    @{ Html.RenderAction("Footer", "Home");}
